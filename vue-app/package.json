{
  "name": "@mf-setup/vue-app",
  "version": "0.1.0",
  "main": "dist/index.umd.js",
  "description": "Vue app component",
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:singlespa": "vue-cli-service serve --target lib --formats umd --name @mf-setup/vue-app --filenameHashing false --mode production ./src/main.ts --watch",
    "build": "vue-cli-service build --target lib --formats umd --inline-vue --name @mf-setup/vue-app --filename index --mode production ./src/main.ts",
    "test:unit": "vue-cli-service test:unit --coverage",
    "test:ci": "npm run build && npm run test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "core-js": "^3.8.3",
    "nanoid": "^3.1.30",
    "single-spa-vue": "^2.5.1",
    "vue": "^3.2.13",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.3.3",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0-rc.1",
    "@vue/cli-plugin-eslint": "~5.0.0-rc.1",
    "@vue/cli-plugin-typescript": "~5.0.0-rc.1",
    "@vue/cli-plugin-unit-jest": "~5.0.0-rc.1",
    "@vue/cli-plugin-vuex": "~5.0.0-rc.1",
    "@vue/cli-service": "~5.0.0-rc.1",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "autoprefixer": "^9.8.7",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-vue": "^8.0.3",
    "eslint-plugin-vuejs-accessibility": "^1.1.0",
    "faker": "^5.5.3",
    "jest": "^27.0.5",
    "postcss": "^8.4.4",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2",
    "ts-jest": "^27.0.4",
    "typescript": "~4.1.5",
    "vue-cli-plugin-tailwind": "~2.2.17"
  },
  "license": "UNLICENSED"
}
